pipeline {
    agent any
        stages {
	   stage('one'){
	   	steps {
	   		sh  "cd /var/lib/jenkins/workspace/Declarative-pipeline/"
	   		sh   "python fileformat.py"
	   		}
		}
	
            stage('Two') {
	          agent {
        	        label "slave"
    		          }
           	 steps {
            		echo 'hello'
			          sh "hostname"
            		}
            		}
            }
    }
